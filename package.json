{
  "name": "nuxtify",
  "version": "1.0.0",
  "description": "Nuxt and Vuetify Starter",
  "author": "Eko Eryanto",
  "private": true,
  "scripts": {
    "start": "nuxt",
    "build": "nuxt build",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "serve": "nuxt build && nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "engines": {
    "node": "8.9",
    "npm": "5.3",
    "yarn": "1.3"
  },
  "dependencies": {
    "firebase": "^5.0.3",
    "jquery": "^3.3.1",
    "mdi": "^2.2.43",
    "mini-toastr": "^0.8.1",
    "nuxt": "^1.0.0",
    "vue-notifications": "^0.9.0",
    "vue-resource": "^1.5.0",
    "vuefire": "^2.0.0-alpha.11",
    "vuetify": "^1.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-imports": "^1.4.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  }
}
